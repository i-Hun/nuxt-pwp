    "from datetime import datetime, timedelta\n",
    "\n",
    "def increment_date(row):\n",
    "    date = datetime.strptime(row['date entered'], \"%Y-%m-%d\")\n",
    "    return date + timedelta(7) * (row['week'] - 1)\n",
    "\n",
    "molten['date'] = molten.apply(increment_date, axis=1)\n",
